images {
   image, "azbooka.png" COMP;
}

collections {
   group {
      name, "background";
      parts {
         part {
            name, "background";
            type, RECT;
            description {
               state, "default" 0.0;
               color, 255 255 255 255;
               rel1 { relative, 0.0 0.0; offset, 0 0; }
               rel2 { relative, 1.0 1.0; offset, -1 -1; }
            }
         }
         part {
            name, "logo";
            type, IMAGE;
            description {
               state, "default" 0.0;
               rel1 { relative, 1.0 1.0; offset, -100 -156; }
               rel2 { relative, 1.0 1.0; offset, -6 -6; }
               image {
                  normal, "azbooka.png";
               }
            }
         }
      }
   }
}
